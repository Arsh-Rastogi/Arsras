{
	"name": "partbot",
	"version": "3.0.0",
	"description": "",
	"main": "src/index.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
		"deploy": "npm run test && ./deploy.sh",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"start": "npm run build && node dist/index.js",
		"start:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\" \"nodemon dist/index.js\"",
		"start:prod": "concurrently \"tsc -w --preserveWatchOutput\" \"tsc-alias -w\" \"node dist/index.js\"",
		"test": "npm run lint && npm run build"
	},
	"keywords": [],
	"author": "PartMan7",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^20.4.4",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"concurrently": "^8.2.0",
		"eslint": "^8.45.0",
		"nodemon": "^3.0.1",
		"tsc-alias": "^1.8.7",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"@types/nunjucks": "^3.2.3",
		"axios": "^1.4.0",
		"chokidar": "^3.5.3",
		"discord.js": "^14.11.0",
		"dotenv": "^16.3.1",
		"mongoose": "^7.4.1",
		"nunjucks": "^3.2.4",
		"ps-client": "^3.3.0"
	}
}
