server {
	listen 80;
	server_name cdn.partman.dev;
	root {{PWD}}/src/web/cdn;
	index /index.html;
	error_page 404 /not_found.html;

	add_header Content-Type "charset=utf-8";

	location = /partbotter {
		try_files /index.html =404;
	}

	location /partbotter/ {
		rewrite ^/partbotter/(.*)$ /$1 break;
		try_files $uri $uri.html =404;
	}

	try_files $uri =404;
}
